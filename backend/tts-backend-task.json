{
  "family": "tts-backend-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::681916125840:role/tts-backend-execution-role",
  "containerDefinitions": [
    {
      "name": "tts-backend",
      "image": "681916125840.dkr.ecr.us-east-1.amazonaws.com/tts-backend:latest",
      "essential": true,
      "memory": 512,
      "cpu": 256,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
